			<column name="location_longitude" value="12.35065"/>
			<column name="distance" value="5"/>
			<column name="racetime" value="ca. 00:20:00"/>
			<column name="costs" value="0,00"/>
			<column name="status" value="2"/>
		</insert>
		<insert tableName="Race">
			<column name="event_id" valueNumeric="select id from Event where name = '${2012-firmenlauf-leipzig}'"/>
			<column name="category_id" valueNumeric="select id from Category where name = '${category-strassenlauf}'"/>
			<column name="start_time" valueDate="2012-06-20T19:30:00"/>
			<column name="distance" value="5000"/>
			<column name="racetime" value="ca. 00:20:00"/>
			<column name="note" value="Zeitmessung hat nicht funktioniert"/>
			<column name="status" value="2"/>
		</insert>

		<insert tableName="Event">
			<column name="organizer_id" valueNumeric="select id from Organizer where name = '${organizer-cross-de-luxe}'"/>
			<column name="name" value="${2012-cross-de-luxe}"/>
			<column name="start_date" value="2012-09-30"/>
			<column name="location_name" value="Markkleeberger See"/>
			<column name="location_country_id" valueNumeric="select id from Country where name = '${country-germany}'"/>
			<column name="location_latitude" value="51.25534"/>
			<column name="location_longitude" value="12.42442"/>
			<column name="distance" value="2 x 8"/>
			<column name="racetime" value="01:59:34"/>
			<column name="costs" value="24,00"/>
			<column name="status" value="2"/>
		</insert>
		<insert tableName="Race">
			<column name="event_id" valueNumeric="select id from Event where name = '${2012-cross-de-luxe}'"/>
			<column name="category_id" valueNumeric="select id from Category where name = '${category-hindernislauf}'"/>
