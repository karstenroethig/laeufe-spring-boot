<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
	                    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

	<property name="2012-firmenlauf-leipzig" value="5. Commerzbank Firmenlauf"/>
	<property name="2012-cross-de-luxe" value="3. CROSS DE LUXE"/>

	<!-- =========== -->
	<!-- insert data -->
	<!-- =========== -->
	<changeSet id="2012" author="roethig">

		<!-- ==================== -->
		<!-- Table "Event" (2012) -->
		<!-- ==================== -->
		<insert tableName="Event">
			<column name="organizer_id" valueNumeric="select id from Organizer where name = '${organizer-leipziger-firmenlauf}'"/>
			<column name="name" value="${2012-firmenlauf-leipzig}"/>
			<column name="start_date" value="2012-06-20"/>
			<column name="location_name" value="Sportforum, Leipzig"/>
			<column name="location_country_id" valueNumeric="select id from Country where name = '${country-germany}'"/>
